# we need numbers, so define some numbers
0 = λf.λx.x
succ = λn.λf.λx.(f (n f x))
+ = λm.λn.(m succ n)
* = λm.λn.(n (+ m) 0)
^ = λa.λb.(b a)
1 = (succ 0)
2 = (succ 1)
3 = (succ 2)
4 = (succ 3)
5 = (succ 4)
6 = (succ 5)
7 = (succ 6)
8 = (succ 7)
9 = (succ 8)
10 = (succ 9)
num = λa.λb.λc.(+ (+ (* (* 10 10) a) (* 10 b)) c)

true = λt.λf.t
false = λt.λf.f
if = λp.λa.λb.((p a b) p)
zero? = λn.(n λ_.false true)

Z = λf.((λx.f (λv.((x x) v))) (λx.f (λv.((x x) v))))

(PRINT_BYTE (Z (λf.λx.(f x)) 8))
